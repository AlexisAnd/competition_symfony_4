{% extends 'base.html.twig' %}

{% block body %}
    <main role="main" class="container">

    <h3>Mettre à jour le résultat du match:</h3>

        {% for message in app.flashes('notice') %}
            <div class="flash-notice">
                {{ message }}
            </div>
        {% endfor %}
    <div>
        {{ form_start(form) }}

        {{ form_widget(form) }}

        {{ form_end(form) }}
    </div>
    </main>

{% endblock %}